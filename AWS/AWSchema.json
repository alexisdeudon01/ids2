{
    "$schema": "http://json-schema.org",
    "title": "AWS Resource Schema",
    "type": "object",
    "properties": {
        "Metadata": {
            "type": "object"
        },
        "Entities": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "ARN",
                    "Service",
                    "Region"
                ],
                "properties": {
                    "ARN": {
                        "type": "string"
                    },
                    "Service": {
                        "type": "string"
                    },
                    "Region": {
                        "type": "string"
                    },
                    "EnrichedData": {
                        "type": "object"
                    }
                }
            }
        }
    }
}